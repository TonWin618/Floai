{
  "general": {
    "ApiClientName": "Http",
    "StartWithWindows": true,
    "InitialPositionX": 400,
    "InitialPositionY": 600,
    "InitialWindowHeight": 500,
    "InitialWindowWidth": 300,
    "MessageSaveDirectory": "./logging",
    "ThemeMode": "light",
    "Theme": "deafult_light",
    "IsMarkdownEnabled": true
  },
  "apiClientOptions": {
    "OpenAi": {
      "ApiKeys": [
        "YOUR_API_KEY_1",
        "YOUR_API_KEY_2"
      ]
    },
    "Http": {
      "url": "https://example.com",
      "headers": {
        "Authorization": "Bearer sk-"
      },
      "params": {},
      "historyFormat": "{\"role\": \"${sender}\", \"content\": \"${content}\"},",
      "aiRoleName": "assistant",
      "userRoleName": "user",
      "body": "{\"model\": \"gpt-3.5-turbo\", \"messages\": [${history}{\"role\": \"user\", \"content\": \"${content}\"}], \"temperature\": 0.7}",
      "contentPath": "choices/0/message/content"
    }
  }
}